extends ./pug/layout/layout.pug

block vars
  -var page = 'ideas';
  -var title = 'Ideas';

block content
  section.main-ideas-header
    .container
      .content
        .text
          p.title Генератор идей
          p.desc Найдите идеи для своих постов
        .search
          input(type="text", placeholder="Введите ключевое слово для поиска идей")
          i.fa.fa-search(aria-hidden="true")
  section.main-ideas-btns
    .container
      .btns
        button.btn-blue Доставка
        button.btn-white Приготовить
        button.btn-white Дома
