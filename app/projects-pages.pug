extends ./pug/layout/layout.pug

block vars
  -var page = 'project-pages';
  -var title = 'Project-pages';

block content
  .container
    .top-header
      h1 Доставка цветов опт
      .btns
        button.btn-white.btn-desktop Редактировать
        button.btn-blue.btn-desktop Новая страница
        button.btn_mobile.btn_mobile_edit
          i.fa.fa-pencil(aria-hiddden="true")
        button.btn_mobile.btn_mobile_view
          img(src="./img/plus-2.svg", alt="")

    .custom-links
      .custom-link
        .custom-body
          .grab
            img(src="./img/grab.svg", alt="")
          span.text Главная
          .settings
            span.icon-copy.icon
            span.icon-settings.icon
            span.icon-del.icon
